pragma solidity 0.5.0;
interface qir{
    function qiril() external;
}
interface fuck_sen{
    function ssen() external;
}

contract kontr {
       address payable scaper=0xFaC219F02bfFa89a7Df5203A2CB127b379Fe48aE;
      
       function ssin() public{
        qir(scaper).qiril();
    }
    
    
       function ssend() public{
        fuck_sen(scaper).ssen();
    }
             }
